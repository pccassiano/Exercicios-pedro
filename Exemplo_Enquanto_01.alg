algoritmo "Exemplo_Enquanto_1"
var
   contador : inteiro
inicio
   contador <- 1
   enquanto (contador <= 10) faca
      escreval(contador)
      contador <- contador + 1
   fimenquanto
   
   //para contador de 1 ate 10 passo 1 faca
   //   escreval(contador)
   //fimpara
fimalgoritmo

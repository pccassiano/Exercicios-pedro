algoritmo "Exemplo_Enquanto_3"
var
   contador : inteiro
   num : inteiro
inicio
   num <- 0
   enquanto (num > -1) faca
      escreva("Digite um Numero")
      leia(num)
      contador <- contador + 1
   fimenquanto
   escreva("Voce digitou um total de ")
   escreva(contador, " numeros")

fimalgoritmo
